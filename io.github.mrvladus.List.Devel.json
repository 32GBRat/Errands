{
    "app-id": "io.github.mrvladus.List.Devel",
    "runtime": "org.gnome.Platform",
    "runtime-version": "45",
    "sdk": "org.gnome.Sdk",
    "command": "errands",
    "finish-args": [
        "--device=dri",
        "--socket=wayland",
        "--socket=fallback-x11",
        "--share=ipc",
        "--share=network"
    ],
    "modules": [
        "build-aux/python3-nextcloud-tasks-api.json",
        {
            "name": "errands",
            "buildsystem": "meson",
            "config-opts": [
                "-Dprofile=development"
            ],
            "sources": [
                {
                    "type": "dir",
                    "path": "."
                }
            ]
        }
    ]
}